392  sudo apt-get update
  393  sudo apt-get -y upgrade
  394  python3 -V
  395  sudo apt-get install -y python3-pip
  396  sudo apt-get install build-essential libssl-dev libffi-dev python-dev
  397  sudo apt-get install -y python3-venv
  398  mkdir environments
  399  cd environments
  400  python3 -m venv my_env
  401  ls my_env
  402  source my_env/bin/activate
  403  vi hello.py
  404  nano hello.py
  405  python hello.py 
  406  deactivate

